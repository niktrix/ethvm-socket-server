{
  "name": "ethvm-socket-server",
  "version": "0.0.1",
  "description": "backend socket server for ethvm",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "nodemon",
    "format": "npm run lint:fix && npm run prettier",
    "lint": "tslint -c tslint.json --project .",
    "lint:fix": "tslint -c tslint.json --project . --fix",
    "prettier": "prettier --write src/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enKryptIO/ethvm-socket-server.git"
  },
  "keywords": [
    "ethvm",
    "socketio",
    "node",
    "socket",
    "ethereum"
  ],
  "author": "kvhnuke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enKryptIO/ethvm-socket-server/issues"
  },
  "homepage": "https://github.com/enKryptIO/ethvm-socket-server#readme",
  "dependencies": {
    "@types/json-schema": "^6.0.1",
    "@types/redis": "^2.8.6",
    "bignumber.js": "^5.0.0",
    "convict": "^4.3.0",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-account": "^2.0.4",
    "ethereumjs-vm": "git+https://github.com/kvhnuke/ethereumjs-vm.git",
    "ioredis": "^3.2.2",
    "json-rpc2": "git+https://github.com/kvhnuke/node-jsonrpc2.git",
    "lodash": "^4.17.4",
    "lokijs": "^1.5.1",
    "lru": "^3.1.0",
    "lrucache": "^1.0.3",
    "merkle-patricia-tree": "^2.3.0",
    "node-fetch": "^2.1.2",
    "pino": "^4.17.3",
    "rethinkdb": "^2.3.3",
    "socket.io": "^2.0.4",
    "web3": "^1.0.0-beta.34",
    "web3-provider-engine": "^14.0.5",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/bignumber.js": "^4.0.3",
    "@types/ioredis": "^3.2.4",
    "@types/lodash": "^4.14.94",
    "@types/lokijs": "^1.5.0",
    "@types/lowdb": "^0.15.0",
    "@types/node": "^8.5.1",
    "@types/node-fetch": "^2.1.1",
    "@types/pino": "^4.16.0",
    "@types/socket.io": "^1.4.31",
    "@types/yargs": "^10.0.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-node6": "^11.0.0",
    "bluebird": "^3.5.1",
    "events": "^1.1.1",
    "husky": "^1.0.0-rc.9",
    "nodemon": "^1.17.5",
    "prettier": "1.13.5",
    "tls": "0.0.1",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-consistent-codestyle": "^1.13.1",
    "typescript": "^2.8.4"
  },
  "optionalDependencies": {
    "@app/config": "tslint-import-local-hack",
    "@app/datastores": "tslint-import-local-hack",
    "@app/helpers": "tslint-import-local-hack",
    "@app/interfaces": "tslint-import-local-hack",
    "@app/libs": "tslint-import-local-hack",
    "@app/models": "tslint-import-local-hack",
    "@app/server": "tslint-import-local-hack",
    "@app/vm": "tslint-import-local-hack"
  }
